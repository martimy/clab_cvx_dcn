auto lo
iface lo inet loopback
    address 10.255.255.4/32

auto eth0
iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    address 127.0.0.1/8
    address ::1/128
    vrf-table auto

auto swp11
iface swp11
  address 10.0.0.66
  netmask 255.255.255.224

auto swp12
iface swp12
  address 10.0.0.162
  netmask 255.255.255.224

auto swp1
iface swp1
  address 10.0.20.1
  netmask 255.255.255.0
